<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/doctor_name_button"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="144dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/ic_rounded_button"
        android:text="Ok"
        android:textColor="#fff"
        app:layout_constraintBottom_toBottomOf="@+id/doctor_name_appointment_details2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toEndOf="@+id/doctor_name_appointment_details2"
        app:layout_constraintTop_toTopOf="@+id/doctor_name_appointment_details"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/doctor_name_appointment_details"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Doctor Name:"
        android:textColor="@color/colorPrimary"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/doctor_name_appointment_details2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Doctor Name"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/patient_name_appointment_details"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doctor_name_appointment_details"
        app:layout_constraintVertical_bias="0.312" />

    <TextView
        android:id="@+id/patient_name_appointment_details"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="Patient Name:"
        android:textSize="20dp"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/doctor_name_appointment_details"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/doctor_name_appointment_details"
        app:layout_constraintTop_toBottomOf="@+id/doctor_name_appointment_details" />

    <TextView
        android:id="@+id/patient_name_appointment_details2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Patient Name"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/date_appointment_details"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/doctor_name_appointment_details"
        app:layout_constraintTop_toBottomOf="@+id/patient_name_appointment_details"
        app:layout_constraintVertical_bias="0.25" />

    <TextView
        android:id="@+id/date_appointment_details"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-12px"
        android:layout_marginTop="72dp"
        android:text="Date of Slot:"
        android:textColor="@color/colorPrimary"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/patient_name_appointment_details"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/patient_name_appointment_details"
        app:layout_constraintTop_toBottomOf="@+id/patient_name_appointment_details" />

    <TextView
        android:id="@+id/date_appointment_details2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Date of Slot"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/patient_name_appointment_details"
        app:layout_constraintTop_toBottomOf="@+id/date_appointment_details" />

    <TextView
        android:id="@+id/start_time_appointment_details"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Start time"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/end_time_appointment_details"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/date_appointment_details2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/end_time_appointment_details"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:text="end Time"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/prescription_appointment_diagnosis"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/date_appointment_details"
        app:layout_constraintVertical_bias="0.578" />

    <EditText
        android:id="@+id/prescription_appointment_drugs"
        android:layout_width="291dp"
        android:layout_height="50dp"
        android:layout_marginTop="44dp"
        android:layout_marginBottom="33dp"
        android:hint="Drugs"
        app:layout_constraintBottom_toTopOf="@+id/view_appointment_layout_button1"
        app:layout_constraintEnd_toEndOf="@+id/end_time_appointment_details"
        app:layout_constraintHorizontal_bias="0.013"
        app:layout_constraintStart_toStartOf="@+id/start_time_appointment_details"
        app:layout_constraintTop_toBottomOf="@+id/prescription_appointment_diagnosis" />

    <EditText
        android:id="@+id/prescription_appointment_diagnosis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="168dp"
        android:ems="10"
        android:hint="Diagnosis"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/end_time_appointment_details"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/start_time_appointment_details" />

    <Button
        android:id="@+id/view_appointment_layout_button1"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_rounded_button"
        android:text="Ok"
        android:textColor="#fff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/prescription_appointment_drugs"
        app:layout_constraintHorizontal_bias="0.198"
        app:layout_constraintStart_toStartOf="@+id/prescription_appointment_drugs"
        app:layout_constraintTop_toBottomOf="@+id/end_time_appointment_details"
        app:layout_constraintVertical_bias="0.834" />

    <Button
        android:id="@+id/view_appointment_layout_button2"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_rounded_button"
        android:text="Ok"
        android:textColor="#fff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.638"
        app:layout_constraintStart_toStartOf="@+id/prescription_appointment_drugs"
        app:layout_constraintTop_toTopOf="@+id/prescription_appointment_drugs"
        app:layout_constraintVertical_bias="0.656" />
</androidx.constraintlayout.widget.ConstraintLayout>