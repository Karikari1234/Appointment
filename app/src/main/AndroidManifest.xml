<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app_ointmentt">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.patient.HomePagePatient"></activity>
        <activity android:name=".ui.doctor.HomePageDoctor" />
        <activity android:name=".ui.doctor.LoginDoctor" android:theme="@style/SplashTheme"/>
        <activity android:name=".ui.patient.LoginPatient" android:theme="@style/SplashTheme"/>
        <activity android:name=".ui.doctor.RegisterDoctor" android:theme="@style/SplashTheme"/>
        <activity android:name=".ui.patient.RegisterPatient" android:theme="@style/SplashTheme"/>
        <activity android:name="app_ointmentt.ui.MainActivity" android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>